<ul class="post-list">
  {% for post in postslist %}
    <li>
      <strong>
        <a class="post-list-title" href="{{ post.url | url }}">{{ post.data.title }}</a>
      </strong>
      -
      <span class="post-list-desc">
        {{ post.data.description }}
      </span>
    </li>
  {% endfor %}
</ul>